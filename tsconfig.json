{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/app/_layout.jsx",
    "src/app/(onboarding)/_layout.jsx",
    "src/app/(onboarding)/onboarding.jsx",
    "src/app/(tabs)/_layout.jsx",
    "src/app/(tabs)/index.jsx",
    "src/app/(tabs)/more.jsx",
    "src/app/(tabs)/qibla.jsx",
    "src/app/(tabs)/settings.jsx",
    "src/app/(extras)/_layout.jsx",
    "src/app/(extras)/abdesi.jsx",
    "src/app/(extras)/about.jsx",
    "src/app/(extras)/namazi.jsx",
    "src/app/(extras)/tesbih.jsx",
  ]
}