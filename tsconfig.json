{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/_layout.jsx",
    "app/index.jsx",
    "app/extras/_layout.jsx",
    "app/extras/abdesi.jsx",
    "app/extras/about.jsx",
    "app/extras/namazi.jsx",
    "app/extras/tesbih.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/home.jsx",
    "app/(tabs)/more.jsx",
    "app/(tabs)/qibla.jsx",
    "app/(tabs)/settings.jsx"
  ]
}